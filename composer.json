{
    "name": "magutti/magutti-spatial",
    "description": "Laravel Builder Mysql Spatial Extension",
    "keywords": [
        "laravel",
        "mysql-spatial"
    ],
    "homepage": "https://github.com/marcoax/maguttiSpatialBuilder",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "marco asperti",
            "email": "marco@magutti.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Magutti\\MaguttiSpatial\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Magutti\\MaguttiSpatial\\MaguttiSpatialServiceProvider"
            ]
        }
    }
}
